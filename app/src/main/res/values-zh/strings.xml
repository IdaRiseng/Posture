    <!-- Movements -->
    <!-- wall -->
    <string name="wall_slide">靠墙滑动</string>
    <string name="wall_slide_desc">确保背部的所有部位都紧贴墙壁。上下滑动手臂，确保手臂始终接触墙壁。</string>
    <string name="wall_touch">靠墙触摸</string>
    <string name="door_frame_stretch">门框拉伸</string>

    <!-- Sitting -->
    <string name="chest_opener">胸部扩展</string>
    <string name="chest_opener_desc">将肘部向侧面和背部打开，肩胛骨挤压在一起</string>
    <string name="neck_stretch">颈部拉伸</string>
    <string name="neck_stretch_desc">把手放在头上。轻轻拉动头部向一侧，以拉伸颈部。</string>
    <string name="heart_opener">心脏扩展</string>
    <string name="heart_opener_desc">把手放在背后，向上拉手臂</string>
    <string name="lower_back_stretch">下背部拉伸</string>
    <string name="lower_back_stretch_desc">坐在椅子上，左手抓住椅子靠背，将上半身向手臂方向拉动。</string>
    <string name="back_stretch">背部拉伸</string>
    <string name="back_stretch_desc">抱住双腿，尽量将背部抬高。</string>
    <string name="turtle_neck">乌龟脖子</string>
    <string name="turtle_neck_desc">用手托住下巴，将头部向后推，确保保持头部正直。</string>
    <string name="angel_wings">天使翅膀</string>
    <string name="angel_wings_desc">双手靠在一堵看不见的墙上，手臂向上，肘部向下。向下推时，注意集中力量于背部肌肉。</string>
    <string name="shoulder_circles">肩膀绕圈</string>

    <!-- Laying -->
    <string name="all_four_twist">四肢扭转</string>
    <string name="all_four_twist_desc">扭转上半身，将一只手臂向天空举起。向上看，进一步扭转以打开胸部。</string>
    <string name="table_top_lift">桌面提升</string>
    <string name="table_top_lift_desc">掌心朝下推地板，身体向上推至水平，保持一会儿，然后慢慢放下。肩胛骨挤压在一起。</string>
    <string name="glute_bridge">臀桥</string>
    <string name="glute_bridge_desc">臀部尽量抬高，双手指交叉，同时挤压背部和肩胛骨。</string>
    <string name="ab_stretch">腹部拉伸</string>
    <string name="ab_stretch_desc">脸朝下躺在地上。将身体推成海豹姿势，拉伸下背部和腹部。</string>
    <string name="back_squeeze_pulse">背部挤压脉冲</string>
    <string name="back_squeeze_pulse_desc">脸朝下躺在地上。将肘部向后拉，轻轻抬起上半身。肩胛骨挤压在一起。向上推3次，然后重复。</string>
    <string name="back_swim">背泳</string>
    <string name="back_swim_desc">趴在地上，保持头向前看。手臂保持伸直，向上和向后移动。</string>
    <string name="cat_cow">猫牛式</string>
    <string name="cat_cow_desc">四肢着地，将脊椎向上拱起，头部向下。然后将脊椎向下弯曲，头部向上。</string>
    <string name="thoracic_rotation">胸椎旋转</string>
    <string name="thoracic_rotation_desc">四肢着地，用一只手臂放在头后，旋转身体，先向上然后向下。</string>
    <string name="extended_puppy_pose">延展幼犬式</string>
    <string name="extended_puppy_pose_desc">四肢着地，向后推身体，伸展手臂，头部朝向地面。</string>
    <string name="lotus_pose">莲花式</string>
    <string name="lotus_pose_desc">趴在地上，双手放在背后，同时抬起上半身和双腿。</string>
    <string name="y_w_t_s">YWTs</string>
    <string name="y_w_t_s_desc">趴在地上，用手臂分别做出“Y”、“W”、“T”形状。</string>
    <string name="shoulder_dislocations">肩部移位</string>
    <string name="shoulder_dislocations_desc">使用带子、围巾或毯子。握住两端，试着将带子从前向后旋转。</string>

