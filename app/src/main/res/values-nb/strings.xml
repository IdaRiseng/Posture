    <!-- Movements -->
    <!-- wall -->
    <string name="wall_slide">Vegg-skli</string>
    <string name="wall_slide_desc">Sørg for å holde alle deler av ryggen mot veggen. \nSkyv armene opp og ned og sørg alltid for at de alltid berører veggen.</string>
    <string name="wall_touch">Veggberøring</string>
    <string name="door_frame_stretch">Dørrammestrekk</string>

    <!-- Sitting -->
    <string name="chest_opener">Bryst åpner</string>
    <string name="chest_opener_desc">Åpne albuene til siden og bakover og klem skulderbladene sammen</string>
    <string name="neck_stretch">Nakke strekk</string>
    <string name="neck_stretch_desc">Hvil hånden på hodet. Trekk hodet forsiktig til siden for å strekke nakken</string>
    <string name="heart_opener">Hjerte åpnener</string>
    <string name="heart_opener_desc">Legg armene bak ryggen og trekk armene oppover</string>
    <string name="lower_back_stretch">Strekk nedre rygg</string>
    <string name="lower_back_stretch_desc">På en stol, flytt venstre arm til stolens øvre skinne og dra overkroppen mot armen.</string>
    <string name="back_stretch">Rygg strekk</string>
    <string name="back_stretch_desc">Klem bena og press ryggen så høyt du kan</string>
    <string name="turtle_neck">Skilpadde nakke</string>
    <string name="turtle_neck_desc">Hold haken og skyv hodet bakover. Sørg for å holde hodet rett</string>
    <string name="angel_wings">Englevinger</string>
    <string name="angel_wings_desc">Legg hendene mot en usynlig vegg, hendene opp og albuene ned. Når du presser ned, sørg for å fokusere på ryggmusklene.</string>
    <string name="shoulder_circles">Skuldersirkler</string>

    <!-- Laying -->
    <string name="all_four_twist">Vri på alle fire</string>
    <string name="all_four_twist_desc">Vri overkroppen for å nå en arm opp mot himmelen. Se opp og vri for å åpne brystet enda mer.</string>
    <string name="table_top_lift">Bordløfter</string>
    <string name="table_top_lift_desc">Skyv håndflatene mot gulvet, med forsiden opp. Skyv kroppen din oppover til den er vannrett, hold og gå sakte ned igjen. Klem skulderbladene sammen</string>
    <string name="glute_bridge">Glutebro</string>
    <string name="glute_bridge_desc">Hofter så høyt opp som mulig. Knip fingrene sammen mens du klemmer ryggen og skulderbladene sammen</string>
    <string name="ab_stretch">Ab strekning</string>
    <string name="ab_stretch_desc">Legg deg med ansiktet ned på bakken. Skyv kroppen opp i en forseglet stilling for å strekke nedre rygg og mage</string>
    <string name="back_squeeze_pulse">Ryggklempuls</string>
    <string name="back_squeeze_pulse_desc">Legg deg med ansiktet ned på bakken. Trekk albuene bakover og løft torso lett. Klem skulderbladene sammen. Skyv opp og ned i 3 reps og gjenta deretter </string>
    <string name="back_swim">Ryggsvømming</string>
    <string name="back_swim_desc">Legg deg på magen mens du holder hodet rett foran deg. Flytt dem opp og tilbake mens du holder dem rett.</string>
    <string name="cat_cow">Katt ku</string>
    <string name="cat_cow_desc">På alle fire skyver du ryggraden mot himmelen og holder hodet nede. Skyv deretter ryggraden mot bakken og hodet opp</string>
    <string name="thoracic_rotation">Thorax rotasjon</string>
    <string name="thoracic_rotation_desc">På alle fire, ta en arm bak hodet og roter kroppen først mot himmelen, deretter bakken. </string>
    <string name="extended_puppy_pose">Utvidet valpestilling</string>
    <string name="extended_puppy_pose_desc">På alle fire, press kroppen bakover for å strekke armene. Gå mot bakken. </string>
    <string name="lotus_pose">Lotus positur</string>
    <string name="lotus_pose_desc">Mens du legger deg på magen, legg hendene bak ryggen og skyv overkroppen og bena mot himmelen. </string>
    <string name="y_w_t_s">YWTs</string>
    <string name="y_w_t_s_desc">Legg deg på magen og lag en Y med armene, deretter W, deretter T.</string>
    <string name="shoulder_dislocations">Skulderluksasjoner</string>
    <string name="shoulder_dislocations_desc">Bruk et bånd, skjerf eller teppe. Hold hver ende og prøv å rotere båndet fra forsiden til baksiden. </string>

